{
  "compilerOptions": {
    "name": "milkup",
    "target": "ES2019",
    "lib": ["esnext", "dom", "dom.iterable"],
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "downlevelIteration": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "typeRoots": ["node_modules/@types", "libdefs/globals"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "paths": {
      "milkup": ["./packages/milkup/src/index.ts"],
      "@milkup/equations": ["./packages/milkup-equations/src/index.ts"]
    }
  }
}
